<%- include('partials/header') %> <%- include('partials/nav') %>

<header>
	<div class="container">
		<table class="table">
			<thead>
				<tr>
					<th scope="col">#</th>
					<th scope="col">Book Name</th>
					<th scope="col">Ratings (10)</th>
				</tr>
			</thead>
			<tbody>
				<% for(let i in books) { %>
				<tr>
					<th scope="row">1</th>
					<td><%= books[i].name %></td>
					<td><%= books[i].rating %></td>
				</tr>
				 
        <% } %>
			</tbody>
		</table>
	</div>
</header>

<%- include('partials/footer') %>
